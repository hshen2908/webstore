<header class="text-white dark:text-white z-10">
    <section class="max-w-full mx-auto flex justify-center items-center">
        <nav class="flex-grow">
            <ul class="flex justify-between">
                <li class="flex">
                    <a href="/contact" class="text-2xl font-medium py-4 px-8 align-middle">
                        <h2>
                            CONTACT
                        </h2>
                    </a>
                </li>
                <li class="flex">
                    <a href="/" class="text-2xl font-bold p-4 align-middle">
                        <h1>
                            HREyewear
                        </h1>
                    </a>
                </li>
                <% if (isAuthenticated) { %>
                    <li class="flex">
                        <a href="/logout" class="text-2xl font-medium py-4 px-8 align-middle">
                            LOG OUT
                        </a>
                    </li>
                <% } else { %>
                    <li class="flex">
                        <a href="/login" class="text-2xl font-medium py-4 px-14 align-middle">
                            LOG IN
                        </a>
                    </li>
                <% } %>
            </ul>
        </nav>
    </section>
</header>