        <tr class="border-b dark:border-b-gray-300 border-b-gray-700">
            <th class="text-left pb-4">Profile Picture</th>
            <th class="text-left pb-4">Name</th>
            <th class="text-left pb-4">Email</th>
            <th class="text-left pb-4">Verified</th>
            <th class="text-left pb-4">Created At</th>
